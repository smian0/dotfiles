{
  "mcpServers": {
    "zen": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "--directory",
        "/Users/smian/github/zen-mcp-server",
        "run",
        "server.py"
      ]
    },
    "serena": {
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "desktop-app",
        "--enable-web-dashboard",
        "false",
        "--enable-gui-log-window",
        "false"
      ],
      "command": "uvx",
      "description": "Semantic code analysis & project memory - IDE-optimized, excludes redundant tools, symbol operations",
      "purpose": "code-analysis",
      "scope": "project"
    },
    "privacy_pdf": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "reloaderoo",
        "proxy",
        "--",
        "python3",
        "/Users/smian/.claude/mcp_servers/privacy_pdf/server.py"
      ],
      "description": "Privacy-preserving PDF extraction with MinerU + PII redaction for safe LLM analysis (auto-restart enabled)",
      "env": {
        "MCPDEV_PROXY_AUTO_RESTART": "true"
      }
    },
    "zai-mcp-server": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@z_ai/mcp-server"
      ],
      "env": {
        "Z_AI_API_KEY": "a24f09170ffc4a16958a9986c475561b.MWYL7fnGDHQesuuR",
        "Z_AI_MODE": "ZAI"
      },
      "description": "Z.AI multimodal analysis (image/video) and AI-powered tools"
    },
    "web-search-prime": {
      "type": "http",
      "url": "https://api.z.ai/api/mcp/web_search_prime/mcp",
      "headers": {
        "Authorization": "Bearer a24f09170ffc4a16958a9986c475561b.MWYL7fnGDHQesuuR"
      },
      "description": "Z.AI web search with content size control and domain filtering"
    },
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "command": "npx",
      "description": "Official library documentation lookup - curated docs, framework patterns",
      "env": {},
      "purpose": "documentation",
      "scope": "library",
      "type": "stdio"
    }
  }
}