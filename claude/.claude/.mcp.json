{
  "mcpServers": {
    "MCP_DOCKER": {
      "args": [
        "mcp",
        "gateway",
        "run"
      ],
      "command": "docker"
    },
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "command": "npx",
      "env": {},
      "type": "stdio"
    },
    "linear": {
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.linear.app/sse"
      ],
      "command": "npx"
    },
    "magic": {
      "args": [
        "-y",
        "@21st-dev/magic"
      ],
      "command": "npx",
      "env": {},
      "type": "stdio"
    },
    "playwright": {
      "args": [
        "@playwright/mcp@latest"
      ],
      "command": "npx"
    },
    "sequential-thinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx",
      "env": {},
      "type": "stdio"
    },
    "serena": {
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--enable-web-dashboard",
        "false",
        "--enable-gui-log-window",
        "false"
      ],
      "command": "uvx"
    },
    "time": {
      "args": [
        "mcp-server-time"
      ],
      "command": "uvx",
      "type": "stdio"
    }
  }
}