{
  "mcpServers": {
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "command": "npx",
      "description": "Official library documentation lookup - curated docs, framework patterns",
      "env": {},
      "purpose": "documentation",
      "scope": "library",
      "type": "stdio"
    },
    "mcp_markdown": {
      "args": [
        "reloaderoo",
        "proxy",
        "--",
        "python3",
        "/Users/smian/dotfiles/claude/.claude/mcp_servers/mcp_markdown/server.py"
      ],
      "command": "npx",
      "env": {
        "MCPDEV_PROXY_AUTO_RESTART": "true"
      },
      "type": "stdio"
    },
    "serena": {
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--enable-web-dashboard",
        "false",
        "--enable-gui-log-window",
        "false"
      ],
      "command": "uvx",
      "description": "Semantic code analysis & project memory - IDE-optimized, excludes redundant tools, symbol operations",
      "purpose": "code-analysis",
      "scope": "project"
    },
    "zen": {
      "args": [
        "reloaderoo",
        "proxy",
        "--",
        "/Users/smian/github/zen-mcp-server/.zen_venv/bin/python",
        "/Users/smian/github/zen-mcp-server/server_proxy.py"
      ],
      "command": "npx",
      "env": {
        "MCPDEV_PROXY_AUTO_RESTART": "true"
      },
      "type": "stdio"
    }
  }
}